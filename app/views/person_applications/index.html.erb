<p id="notice"><%= notice %></p>

<%= render "shared/api_response" %>

<h1>Person Applications</h1>

<table>
  <thead>
    <tr>
      <th>Treasury Prime Application Id</th>
      <th>Date of birth</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone number</th>
      <th>City</th>
      <th>State</th>
      <th>Postal code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @person_applications.each do |person_application| %>
      <tr>
        <td><%= person_application.treasury_prime_id %></td>
        <td><%= person_application.date_of_birth %></td>
        <td><%= person_application.first_name %></td>
        <td><%= person_application.last_name %></td>
        <td><%= person_application.phone_number %></td>
        <td><%= person_application.city %></td>
        <td><%= person_application.state %></td>
        <td><%= person_application.postal_code %></td>
        <td><%= link_to 'Show', person_application %></td>
        <td><%= link_to 'Edit', edit_person_application_path(person_application) %></td>
        <td><%= link_to 'Destroy', person_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person Application', new_person_application_path %>

